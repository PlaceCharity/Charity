{
  "name": "waiter",
  "version": "1.0.0",
  "private": "true",
  "module": "src/main.ts",
  "exports": "./src/main.ts",
  "scripts": {
    "check": "tsc --noEmit && bun run db:check",
    "dev": "bun run --watch src/main.ts",
    "start": "bun run src/main.ts",
    "db:push": "drizzle-kit push",
    "db:migration:generate": "drizzle-kit generate",
    "db:migration:apply": "bun run src/instance/database/migrate.ts",
    "db:check": "drizzle-kit check",
    "db:studio": "drizzle-kit studio"
  },
  "paths": {
    "~/*": ["./src/*"]
  },
  "dependencies": {
    "@auth/core": "^0.28.1",
    "@auth/drizzle-adapter": "^0.8.1",
    "@aws-sdk/client-s3": "3.598.0",
    "@aws-sdk/s3-request-presigner": "^3.598.0",
    "@elysiajs/cors": "^1.0.2",
    "@elysiajs/swagger": "^1.0.3",
    "@t3-oss/env-core": "^0.9.2",
    "@types/uuid": "^9.0.8",
    "drizzle-orm": "^0.30.4",
    "elysia": "^1.0.22",
    "uuid": "^9.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@elysiajs/eden": "^1.0.13",
    "better-sqlite3": "^9.4.3",
    "bun-types": "latest",
    "drizzle-kit": "^0.21.4"
  },
  "trustedDependencies": [
    "better-sqlite3",
    "drizzle-kit"
  ]
}